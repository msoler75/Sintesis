{
  "type": "module",
  "name": "sintesis",
  "version": "1.0.0",
  "description": "Lenguaje de programación para aprendizaje",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "generate": "cp ./src/SintesisParserBase.js ./src/lib && java -Dfile.encoding=UTF-8 -cp D:/projects/juego-programacion/sintesis/antlr4/jar/antlr-4.9.3-complete.jar org.antlr.v4.Tool -Dlanguage=JavaScript -lib ./src ./src/grammar/SintesisLexer.g4 ./src/grammar/SintesisParser.g4 -o ./src/lib -no-listener -visitor && node ./to_async",
    "cp": "cp -v -r -u ./src ../nuxt2/Sintesis",
    "eval": "node ./index.js",
    "generatejava": "java  -cp D:/projects/juego-programacion/sintesis/antlr4/jar/antlr-4.9.3-complete.jar org.antlr.v4.Tool -Dlanguage=Java -lib ./src ./src/grammar/SintesisLexer.g4 ./src/grammar/SintesisParser.g4 -o ./src/lib -no-listener -visitor",
    "javacompile": "javac  -cp D:/projects/juego-programacion/sintesis/antlr4/jar/antlr-4.9.3-complete.jar ./src/lib/*.java",
    "j" : "yarn generatejava && yarn javacompile",
    "setup": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command ./sintesis/setup.ps1"
  },
  "keywords": [
    "antlr4"
  ],
  "author": "Marcel Soler / Pigmalión",
  "license": "ISC",
  "dependencies": {
    "antlr4": "^4.9.3",
    "fs": "^0.0.1-security",
    "jest": "^27.4.7",
    "prompt-sync": "^4.2.0"
  },
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/(?!variables/.*)"
    ],
    "setupFilesAfterEnv": [
      "./setupJest.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.4.6"
  }
}
