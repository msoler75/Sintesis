{
  "type": "module",
  "name": "sintesis",
  "version": "1.0.0",
  "description": "Lenguaje de programación para aprendizaje",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "npx mocha",
    "generate": "cp ./src/SintesisParserBaseSource.js ./src/lib/SintesisParserBase && java -Dfile.encoding=UTF-8 -cp D:/projects/CodeBounce/Sintesis/antlr4/jar/antlr-4.9.3-complete.jar org.antlr.v4.Tool -Dlanguage=JavaScript -lib ./src ./src/grammar/SintesisLexer.g4 ./src/grammar/SintesisParser.g4 -o ./src/lib -no-listener -visitor",
    "toasync": "node ./to_async",
    "tosync": "node ./to_sync",
    "cp": "cp -v -r -u ./src ../cb_nuxt3/Sintesis",
    "eval": "node ./index.js",
    "generatejava": "java  -cp D:/projects/juego-programacion/sintesis/antlr4/jar/antlr-4.9.3-complete.jar org.antlr.v4.Tool -Dlanguage=Java -lib ./src ./src/grammar/SintesisLexer.g4 ./src/grammar/SintesisParser.g4 -o ./src/lib -no-listener -visitor",
    "javacompile": "javac  -cp D:/projects/juego-programacion/sintesis/antlr4/jar/antlr-4.9.3-complete.jar ./src/lib/*.java",
    "j": "yarn generatejava && yarn javacompile",
    "setup": "@powershell -NoProfile -await executionPolicy Unrestricted -Command ./sintesis/setup.ps1"
  },
  "keywords": [
    "antlr4"
  ],
  "author": "Marcel Soler / Pigmalión",
  "license": "ISC",
  "dependencies": {
    "antlr4": "^4.9.3",
    "chai": "^4.3.7",
    "fs": "^0.0.1-security",
    "prompt-sync": "^4.2.0",
    "safe-eval": "^0.4.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-assertions": "^7.17.12",
    "@babel/preset-env": "^7.16.11",
    "babel-jest": "^27.5.1",
    "mocha": "^10.2.0"
  }
}
