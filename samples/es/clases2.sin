// clase sencilla sin atributos

clase animal {

    métodos:

    método correr() { 
        imprimir "corriendo..."
    }

    método saltar() {
        retornar "saltando..."
    }                                                                                                           

}

a = nuevo animal()
a.correr()
imprimir a.saltar()

// vemos la clase
imprimir a
imprimir a.atributos // no tiene atributos
imprimir a.métodos
